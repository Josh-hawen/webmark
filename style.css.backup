.pluses {
    height: 200px;
    width: 100%;
    margin-top: 15px;
    margin-bottom: 15px;
}

.plus1 {
    background: url('https://www.pravoservice.com.ua/wp-content/uploads/2017/08/1.jpg') no-repeat center top;
}

.plus2 {
    background: url('https://www.pravoservice.com.ua/wp-content/uploads/2017/08/2.jpg') no-repeat center top;

}

.plus3 {
    background: url('https://www.pravoservice.com.ua/wp-content/uploads/2017/08/3.jpg') no-repeat center top;
    
}

.plus4 {
    background: url('https://www.pravoservice.com.ua/wp-content/uploads/2017/08/4.jpg') no-repeat center top;
    
}

.plus5 {
    background: url('https://www.pravoservice.com.ua/wp-content/uploads/2017/08/5.jpg') no-repeat center top;
    
}

.plus1, .plus2, .plus3, .plus4, .plus5 {
    background-size: 100% 100%;
    background-position: top;
    height: 200px;
    width: 200px;
    margin: 0 auto;
    float: left;
    margin-right: 17px;
    background-repeat: no-repeat;
}

.tooltip {
width: 100px;
}

.pluses span {
    line-height: 26px;
    color: #fff;
    font-weight: 700;
    text-align: center;
    font-size: 16px;
    display: block;
	text-transform: uppercase;
}

.pluses p{
    display: none;
    font-size: 18px;
    font-weight: 700;
    font-family: 'Segoe UI';
