    text-align: center;
    color: #fff;
}


.plus1:hover, .plus2:hover, .plus3:hover, .plus4:hover, .plus5:hover {
    border-radius: 5px;
    
    
}

.plus1:hover p, .plus2:hover p, .plus3:hover p, .plus4:hover p, .plus5:hover p{
