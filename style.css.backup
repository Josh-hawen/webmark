.main-menu > li.current-menu-item > .sub-menu > li,
.main-menu > li.current-menu-ancestor > .sub-menu > li {
  /*border: 1px solid black;*/
  /*list-style-type: none;*/
  background-color: #D3D3D5;
  flex: 0 0 276px;
  margin: 0 5px;
  position: relative; }

#next_desktop,
#prev_desktop {
  position: absolute;
  top: 106px;
  /*background-color: red;*/
  height: 122px;
  width: 50px;
  z-index: 10;
  transition: .15s;
  background-size: auto 100px;
  background-repeat: no-repeat;
  background-position: center center;
  cursor: pointer; }

#next_desktop:hover {
  background-image: url(img/menu/mobile/button-right_h.png); }

#prev_desktop:hover {
  background-image: url(img/menu/mobile/button-left_h.png); }

.main-menu > li > ul > li.current-menu-item > ul,
.main-menu > li > ul > li.current-menu-ancestor > ul {
  text-align: center;
  position: relative;
  padding-top: 2px; }

.main-menu > li.current-menu-item > a:before,
.main-menu > li.current-menu-ancestor > a:before {
  content: '';
  display: none; }

#next_desktop.active,
#prev_desktop.active {
  opacity: .5; }

#next_desktop {
  right: -55px;
  background-image: url(img/menu/mobile/button-right.png); }

#prev_desktop {
  left: -55px;
  background-image: url(img/menu/mobile/button-left.png); }

.main-menu > li > a {
  color: #333;
  border-radius: 0;
  padding: 15px 12px;
  border-right: none;
  border-left: none;
  border-bottom: none;
  width: 50%; }

.main-menu > li:first-child > a {
  border-right: 1px solid #1c0a09 !important; }

.main-menu > li.current-menu-item > a,
.main-menu > li > a:hover,
.main-menu > li.current-menu-ancestor > a {
  color: #FFFFFF;
  border-color: #2eb5f3;
  background: #2eb5f3;
  text-decoration: none; }

.main-menu > li > ul > li.menu-item > .sub-menu,
.main-menu > li > ul > li.menu-item > .sub-menu {
  display: block !important;
  padding-top: 3px; }

